{
  "private": false,
  "name": "tasty-midway",
  "version": "1.0.0",
  "author": "Will Streeter <w.streeter@tastymakers.io>",
  "scripts": {
    "deploy": "npm run graphql-deploy && npm run sanity-deploy",
    "format": "prettier-eslint --write \"**/*.js\" \"!node_modules/**\"",
    "graphql-deploy": "sanity graphql deploy --playground",
    "sanity-deploy": "sanity deploy",
    "lint": "eslint .",
    "start": "sanity start",
    "build": "sanity build",
    "now-build": "npm run build",
    "now-dev": "npm run build",
    "sync:export": "sanity dataset export production",
    "sync:import": "sanity dataset import production.tar.gz staging --replace",
    "sync:cleanup": "rm production.tar.gz",
    "test": "sanity check"
  },
  "dependencies": {
    "@sane-shopify/sanity-plugin": "^0.18.0",
    "@sanity/base": "^2.0.9",
    "@sanity/cli": "^2.0.9",
    "@sanity/color-input": "^2.0.9",
    "@sanity/components": "^2.0.9",
    "@sanity/core": "^2.0.9",
    "@sanity/default-layout": "^2.0.9",
    "@sanity/default-login": "^2.0.9",
    "@sanity/desk-tool": "^2.0.9",
    "@sanity/image-url": "^0.140.19",
    "@sanity/vision": "^2.0.9",
    "@xstyled/styled-components": "^1.19.1",
    "a11y-react-emoji": "^1.1.2",
    "get-video-id": "^3.1.5",
    "humanize-list": "^1.0.1",
    "node-emoji": "^1.10.0",
    "prop-types": "^15.7.2",
    "ramda": "^0.27.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-icons": "^3.11.0",
    "sanity-plugin-media": "^0.4.0",
    "sanity-plugin-seo-tools": "^1.0.6",
    "sanity-plugin-tabs": "^2.0.1",
    "sanity-typed-queries": "^0.7.3"
  },
  "devDependencies": {
    "@good-idea/unwind-edges": "^1.5.3",
    "@types/prop-types": "^15.7.3",
    "@types/react": "^16.9.55",
    "@types/react-dom": "^16.9.8",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.18.0",
    "eslint-plugin-standard": "^4.0.1",
    "prettier": "^1.19.1",
    "prettier-eslint-cli": "^5.0.0"
  }
