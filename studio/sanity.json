{
  "root": true,
  "project": {
    "name": "tasty-midway",
    "basePath": "/"
  },
  "api": {
    "projectId": "fhy9rtix",
    "dataset": "production"
  },
  "env": {
    "development": {
      "api": {
        "dataset": "production"
      }
    }
